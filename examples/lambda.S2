map
  = PROC(x, f)
    : SELECT $f($)
        FROM $x;

filt
  = PROC(x, f)
    : SELECT $
        FROM $x
       WHERE $f($);

reduce
  = PROC(x, f, init:0)
    : SELECT $i@$f($) # -> $i = $f($i, $)
        FROM $x
  START WITH $i = $init
          AS void;

